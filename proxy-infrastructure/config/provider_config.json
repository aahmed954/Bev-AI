  "load_balancing": {
    "strategies": {
      "round_robin": {
        "description": "Simple round-robin selection",
        "use_case": "Even distribution"
      },
      "least_connections": {
        "description": "Select proxy with fewest active connections",
        "use_case": "Load balancing"
      },
      "weighted_round_robin": {
        "description": "Weighted selection based on proxy performance",
        "use_case": "Performance optimization"
      },
      "least_response_time": {
        "description": "Select proxy with best response time",
        "use_case": "Latency optimization"
      },
      "geographic": {
        "description": "Geographic proximity based selection",
        "use_case": "Regional optimization"
      },
      "random": {
        "description": "Random proxy selection",
        "use_case": "Simple distribution"
      }
    },
    "default_strategy": "least_connections",
    "failover_strategy": "geographic",
    "health_check_interval": 30,
    "max_retries": 3
  },
  "security": {
    "ssl_verification": true,
    "user_agent_rotation": true,
    "header_randomization": true,
    "connection_pooling": true,
    "request_timeout": 30,
    "max_redirects": 3,
    "enable_cookies": false,
    "dns_over_https": true
  },
  "monitoring": {
    "health_check_urls": [
      "http://httpbin.org/ip",
      "https://api.ipify.org",
      "http://icanhazip.com"
    ],
    "performance_thresholds": {
      "response_time_warning": 2.0,
      "response_time_critical": 5.0,
      "success_rate_warning": 0.9,
      "success_rate_critical": 0.7,
      "utilization_warning": 0.8,
      "utilization_critical": 0.95
    },
    "metrics_retention": {
      "raw_metrics": "24h",
      "aggregated_metrics": "30d",
      "summary_metrics": "1y"
    }
  }
