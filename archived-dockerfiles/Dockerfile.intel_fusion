FROM python:3.11-slim
LABEL maintainer="BEV DevOps"
WORKDIR /app
COPY src/security/intel_fusion.py ./
COPY requirements.txt ./
RUN pip install --no-cache-dir -r requirements.txt
ENV PYTHONUNBUFFERED=1
HEALTHCHECK CMD ["python", "intel_fusion.py", "--healthcheck"]
CMD ["python", "intel_fusion.py"]
EXPOSE 8093
